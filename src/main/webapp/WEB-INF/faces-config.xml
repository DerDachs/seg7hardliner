<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <action-listener>
            org.primefaces.application.DialogActionListener
        </action-listener>
        <navigation-handler>
            org.primefaces.application.DialogNavigationHandler
        </navigation-handler>
        <view-handler>
            org.primefaces.application.DialogViewHandler
        </view-handler>
        <locale-config>
            <default-locale>en_US</default-locale>
            <supported-locale>de_DE</supported-locale>
            <supported-locale>en_US</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>msg</base-name>
            <var>MSG</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <application>
        <el-resolver>de.hspf.hardliner.view.bericht.util.JsfCrudELResolver</el-resolver>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>bericht</managed-bean-name>
        <managed-bean-class>de.hspf.hardliner.view.bericht.BerichtController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>berichtJpa</managed-bean-name>
        <managed-bean-class>de.hspf.hardliner.view.bericht.util.BerichtFacade</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <converter>
        <converter-for-class>de.hspf.hardliner.model.Bericht</converter-for-class>
        <converter-class>de.hspf.hardliner.view.bericht.BerichtConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>bericht_create</from-outcome>
            <to-view-id>/bericht/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>bericht_list</from-outcome>
            <to-view-id>/bericht/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>bericht_edit</from-outcome>
            <to-view-id>/bericht/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>bericht_detail</from-outcome>
            <to-view-id>/bericht/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>filiale</managed-bean-name>
        <managed-bean-class>de.hspf.hardliner.view.bericht.FilialeController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>filialeJpa</managed-bean-name>
        <managed-bean-class>de.hspf.hardliner.view.bericht.util.FilialeFacade</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <converter>
        <converter-for-class>de.hspf.hardliner.model.Filiale</converter-for-class>
        <converter-class>de.hspf.hardliner.view.bericht.FilialeConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>filiale_create</from-outcome>
            <to-view-id>/filiale/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>filiale_list</from-outcome>
            <to-view-id>/filiale/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>filiale_edit</from-outcome>
            <to-view-id>/filiale/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>filiale_detail</from-outcome>
            <to-view-id>/filiale/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
